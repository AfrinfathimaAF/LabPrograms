    <script>
        //set-> can't accept duplicates, identify unique values 
        //create set 
        let email=new Set([]);
        email.add("js@gmail.com");
        email.add("java@gmail.com");
        email.add("python@gmail.com");
        console.log(email)
        email.delete("js@gmail.com");
        console.log(email);
        for(let mail of email){
            console.log(mail)
        }
        email.foreach(x=>{
            console.log(x);
        });
        //example
        let adminpermission=new Set(["write","read","delete"])
        let userpermission=new Set(["read"]);
        function haspermission(role,permisssion){
            return role.has(permission);
        }
        console.log(haspermission(adminpermission,"delete"));
        console.log(haspermission(userpermission,"delete"));
        //map
        let user1=(name:"afrin");
        let user2=(name:"thaha");
        let mao=new Map();
        map.set(user1,"welcome");
        map.set(user2,"hello");
        console.log(map);
        map.delete(user2);
        console.log(map);
        for(let details of map.keys){
            console.log(details);
        }

    </script>
